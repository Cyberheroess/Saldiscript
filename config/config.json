{
    "model": {
        "ml_model_path": "data/ml_model.h5", 
        "max_payload_length": 256, 
        "vectorizer": "CountVectorizer",
        "prediction_threshold": 0.7
    },
    "attack_settings": {
        "sql_injection": true,
        "xss_attack": true,
        "ddos_attack": true,
        "file_inclusion": true,
        "csrf_attack": false,
        "reverse_shell": true,
        "web_shell_upload": true,
        "phishing_attack": false
    },
    "proxy_settings": {
        "use_proxy": true,
        "proxy_list_file": "data/proxies.txt"
    },
    "user_agent_settings": {
        "use_user_agent_list": true,
        "user_agents_file": "data/user_agents.txt"
    },
    "logging": {
        "enable_logging": true,
        "log_file": "logs/attack_results.log",
        "log_level": "INFO"
    },
    "encryption": {
        "key": "YOUR_SECRET_KEY_16",
        "encryption_method": "AES",
        "encryption_mode": "CBC"
    },
    "network": {
        "target_ip": "127.0.0.1",
        "target_port": 80
    }
}
